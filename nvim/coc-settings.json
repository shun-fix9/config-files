{
    "languageserver": {
        "efm": {
            "command": "efm-langserver",
            "args": [],
            "filetypes": ["markdown", "css"]
        },
        "rust": {
            "command": "rustup",
            "args": ["run", "stable", "rls"],
            "filetypes": ["rust"]
        },
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
            "filetypes": ["go"],
            "initializationOptions": {
                "usePlaceholders": true
            }
        }
    },
    "typescript.showUnused": false
}
